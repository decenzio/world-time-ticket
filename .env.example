# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# NextAuth Configuration
NEXTAUTH_URL=https://world-time-ticket.vercel.app/
NEXTAUTH_SECRET=
# Other Environment Variables
NEXT_PUBLIC_APP_URL=https://world-time-ticket.vercel.app/

# World ID OAuth Configuration
# Get these from https://developer.world.org
NEXT_PUBLIC_WORLD_ID_ACTION_ID=verify-human
NEXT_PUBLIC_WORLD_ID_APP_ID=app_
WLD_CLIENT_SECRET=sk_

# Optional: Calendly Integration
CALENDLY_CLIENT_ID=your-calendly-client-id
CALENDLY_CLIENT_SECRET=your-calendly-client-secret
# For local development you can use the built-in mock instead of real Calendly.
# Set this to "true" to enable the mock API which returns fake event ids.
# Do NOT enable this in production.
CALENDLY_MOCK=true

# Optional: Blockchain/Smart Contract
NEXT_PUBLIC_ESCROW_CONTRACT_ADDRESS=your-escrow-contract-address
NEXT_PUBLIC_RPC_URL=your-rpc-url
